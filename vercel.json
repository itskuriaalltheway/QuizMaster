{
  "buildCommand": "pnpm run build:client",
  "outputDirectory": "dist/spa",
  "installCommand": "pnpm install --frozen-lockfile",
  "framework": "vite",
  "nodeVersion": "18.x",
  "packageManager": "pnpm@10",
  "git": {
    "deploymentEnabled": true
  },
  "env": {
    "NODE_ENV": "production"
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "status": 200
    }
  ]
}
