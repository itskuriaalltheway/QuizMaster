{
  "buildCommand": "pnpm install && pnpm run build:client",
  "outputDirectory": "dist/spa",
  "framework": "vite",
  "nodeVersion": "18.x",
  "packageManager": "pnpm@10.14.0",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
